!
!
!
hostname R2
no ip domain lookup
ipv6 unicast-routing
!
interface Serial0/1/0
 ip address 10.0.0.2 255.255.255.0
 ipv6 address 1::2/64
 ipv6 ospf 1 area 1
 no shutdown
!
interface GigabitEthernet0/0/0
 ip address 9.1.0.1 255.255.255.0
 ipv6 address 9::1/64
 ipv6 ospf 1 area 1
 no shutdown
!
router ospf 1
 router-id 2.2.2.2
 network 10.0.0.0 0.0.0.255 area 1
 network 9.1.0.1 0.0.0.255 area 1
 !Set BGP connecting interface as OSPF passive
 passive-interface GigabitEthernet0/0/0
!
ipv6 router ospf 1
 router-id 2.2.2.2
 !Set BGP connecting interface as OSPF passive
 passive-interface GigabitEthernet0/0/0
!
!Valid BGP configuration
router bgp 1
 bgp router-id 1.1.1.1
 bgp log-neighbor-changes
 !Disable default IPv4 unicast bgp attributes
 no bgp default ipv4-unicast
 neighbor 9::2 remote-as 2
 neighbor 9.1.0.2 remote-as 2
 !
 address-family ipv4
  network 30.0.0.0 mask 255.255.255.0
  neighbor 9.1.0.2 activate
 exit-address-family
 !
 address-family ipv6
  network 2::/64
  neighbor 9::2 activate
 exit-address-family
 !
!
!
line con 0
 logging synchronous
!
end
